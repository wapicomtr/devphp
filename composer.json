{
    "name": "devsly/php-sdk",
    "description": "Professional PHP SDK for DevSLY.io API - Network Tools, Load Testing, Developer Tools & Code Analysis",
    "type": "library",
    "keywords": ["devsly", "api", "sdk", "network-tools", "load-testing", "developer-tools", "whois", "dns", "ssl"],
    "license": "MIT",
    "authors": [
        {
            "name": "DevSLY Contributors",
            "email": "info@devsly.io"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "ext-curl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "phpstan/phpstan": "^1.0",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "DevSly\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DevSly\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "phpstan": "phpstan analyse src",
        "cs-check": "phpcs src",
        "cs-fix": "phpcbf src"
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
